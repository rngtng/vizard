!!!
%html{lang:"en"}
  %head
    %meta{charset:'utf-8'}
    %meta{'http-equiv' => "X-UA-Compatible", :content => "chrome=1"}
    %meta{name:"viewport", content:"width=device-width, initial-scale=1, maximum-scale=1"}

    %title UML Generator - Graphs generated with PlantUML

    /[if lt IE 9]
      %script{src:"//html5shiv.googlecode.com/svn/trunk/html5.js"}

    %link{rel:"stylesheet", type:"text/css", href:"/css/auth-buttons.css", media:"screen"}
    %link{rel:"stylesheet", type:"text/css", href:"/css/github.css", media:"screen"}
    %link{rel:"stylesheet", type:"text/css", href:"/css/style.css", media:"screen"}
    %link{rel:"stylesheet", type:"text/css", href:"/javascript/fancybox/jquery.fancybox.css?v=2.1.5", media:"screen"}

  %body.flex
    %header
      %h1
        UML Generator - Graphs generated with PlantUML
      .content
        .login
          %a{href:'/login'} login

        .logout
          %a{href:'#'} logout

    .edit.flex.green
      .editor.flex1.blue
        #editor A -> B A -> B

      .view.red
        .content.yellow
          %a.fancybox{href:'#'}
            %img{src:'#'}


    .browse.yellow
      .navigation
        .menu.yellow
          %ul
            %li
              %a{href:'#view1'} 1
            %li
              %a{href:'#view2'} 2
            %li
              %a{href:'#view3'} 3

      .view.red
        .content
          %ul
            %li#view1
              %img{src:'http://planet-terre.ens-lyon.fr/objets/Images/Img122/122-source-du-lison.jpg'}
            %li#view2
              %img{src:'http://planet-terre.ens-lyon.fr/objets/Images/Img122/122-source-du-lison.jpg'}
            %li#view3
              %img{src:'http://planet-terre.ens-lyon.fr/objets/Images/Img122/122-source-du-lison.jpg'}

    %script{src:"/javascript/jquery.min.js"}
    %script{src:"/javascript/jquery.scrollTo-min.js"}
    %script{src:"/javascript/jquery.scrollspy.js"}
    %script{src:"/javascript/jquery.cookie.js"}
    / %script{src:"//brandonaaron.github.io/jquery-mousewheel/jquery.mousewheel.js"}
    %script{src:"/javascript/fancybox/jquery.fancybox.pack.js?v=2.1.5"}
    %script{src:"/javascript/ace/ace.js"}
    %script{src:"/javascript/underscore-min.js"}
    %script{src:"/javascript/github.js"}
    %script{src:"/javascript/application.js"}
